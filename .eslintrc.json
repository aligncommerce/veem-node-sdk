{
  "extends": "standard",
  "globals": {
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "it": true,
    "expect": true,
    "jest": true,
    "beforeAll": true,
    "afterAll": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "no-unused-expressions": 0,
    "no-console": 2,
    "no-debugger": 2,
    "no-unused-vars": 2
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false,
    "ecmaVersion": 6
  },
  "settings": {}
}
