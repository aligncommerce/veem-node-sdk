    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-circus": "^24.7.0",
    "lint-staged": "^8.1.5",
    "moment": "^2.24.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "opn": "^5.5.0",
    "random-email": "^1.0.3",
