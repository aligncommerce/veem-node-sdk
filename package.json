{
  "name": "Veem Node SDK",
  "version": "1.0.0",
  "description": "Node.js Library for the Veem Global Payments API",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest ./lib",
    "test:e2e": "jest ./e2e --runInBand",
    "test:plugins": "jest ./jest/plugins",
